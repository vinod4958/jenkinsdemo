pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'Build the code'
            }
        }
        stage('Test') {
            steps {
                echo 'test the code'
            }
        }
        stage('Unit test') {
            steps {
                echo 'Run unit test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deploy the code'
            }
        }
    }
}
